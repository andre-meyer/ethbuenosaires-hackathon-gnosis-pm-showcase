{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "class-methods-use-this": 0,
    "semi": ["error", "never"],
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "indent": ["error", 2],
    "no-console": ["error", { "allow": ["warn", "error"] }]
  },
  "env": {
    "browser": true
  }
}
